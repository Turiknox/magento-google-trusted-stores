<config>
    <modules>
        <Turiknox_Trustedstores>
            <version>1.0.2</version>
        </Turiknox_Trustedstores>
    </modules>
    <global>
        <blocks>
            <turiknox_trustedstores>
                <class>Turiknox_Trustedstores_Block</class>
            </turiknox_trustedstores>
        </blocks>
        <helpers>
            <turiknox_trustedstores>
                <class>Turiknox_Trustedstores_Helper</class>
            </turiknox_trustedstores>
        </helpers>
        <models>
            <turiknox_trustedstores>
                <class>Turiknox_Trustedstores_Model</class>
            </turiknox_trustedstores>
        </models>
    </global>
    <frontend>
        <layout>
            <updates>
                <turiknox_trustedstores>
                    <file>turiknox/trustedstores.xml</file>
                </turiknox_trustedstores>
            </updates>
        </layout>
    </frontend>
</config>